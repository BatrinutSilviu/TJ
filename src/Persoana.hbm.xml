<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="exemplu.Persoana" table="persoane">
		<id name="ID" type="int">
			<column name="ID" />
		</id>
		<property name="Nume" type="java.lang.String">
			<column name="Nume" />
		</property>
		<property name="Varsta" column="Varsta" type="int" />
	</class>
</hibernate-mapping>